  "license": "ISC",
  "dependencies": {
    "express": "^4.17.3",
    "serverless": "^1.46.1",
    "serverless-http": "^2.0.2",
    "serverless-offline": "^5.7.2",
    "serverless-plugin-typescript": "^1.1.7",
    "ts-node": "^9.1.1",
    "typescript": "^4.4.4"
  }
}