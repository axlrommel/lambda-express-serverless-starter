{
  "name": "sls-express-s3-dynamo",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "lint": "eslint src --fix",
    "dev:dynamo": "npm run dyn:setEndpoint && npm run dyn:copyDefaults && (lsof -ti:8001 | xargs kill) && (npm run dyn:startDynamoLocal & dynamodb-admin)",
    "dyn:setEndpoint": "export DYNAMO_ENDPOINT=http://localhost:8000",
    "dyn:copyDefaults": "cd ./.dynamodb && (cp -n shared-local-instance-blank.db shared-local-instance.db | echo 0)",
    "dyn:startDynamoLocal": "cd ./.dynamodb && (pkill java | echo 0) && (java -Djava.library.path=./DynamoDBLocal_lib -jar DynamoDBLocal.jar -sharedDb | echo 0)",
    "run:local": "serverless offline start",
    "deploy:non-prod": "sls deploy --stage non-prod",
    "deploy:prod": "sls deploy --stage prod",
    "log": "serverless logs -f $FUNCTION -s $STAGE",
    "log:tail": "serverless logs -f $FUNCTION -s $STAGE -t"
  },
  "repository": {
    "type": "git",
    "url": "ssh://git-codecommit.us-east-1.amazonaws.com/v1/repos/sls-express-s3-dynamo"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-source-map-support": "^1.0.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-3": "^6.24.1",
    "eslint": "^5.15.3",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-prettier": "^3.0.1",
    "fetch-mock": "^7.3.1",
    "jest": "^21.2.1",
    "prettier": "^1.16.4",
    "serverless-offline": "^4.9.4",
    "serverless-webpack": "^5.2.0",
    "supertest": "^4.0.2",
    "webpack": "^4.16.2",
    "webpack-node-externals": "^1.6.0"
  },
  "dependencies": {
    "aws-sdk": "^2.437.0",
    "babel-runtime": "^6.26.0",
    "dynamodb-admin": "^3.1.2",
    "express": "^4.16.4",
    "isomorphic-fetch": "^2.2.1",
    "node-jose": "^1.1.3",
    "ramda": "^0.26.1",
    "serverless-http": "^1.9.1",
    "source-map-support": "^0.4.18"
  }
}
